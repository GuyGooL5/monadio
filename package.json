{
  "name": "monadio",
  "version": "0.0.11",
  "description": "A little library that provides useful and smartly-typed monads for TypeScript.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "watch": "tsc -w",
    "build": "pnpm build:js && pnpm build:type",
    "build:js": "node build.js",
    "build:type": "tsc",
    "test": "tsc --noEmit"
  },
  "keywords": [
    "monad",
    "functional",
    "typescript",
    "result",
    "maybe",
    "switch",
    "narrow"
  ],
  "homepage": "https://github.com/GuyGooL5/monadio.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/GuyGooL5/monadio.git"
  },
  "author": "Guy Tsitsiashvili",
  "license": "MIT",
  "packageManager": "pnpm@9.4.0",
  "devDependencies": {
    "esbuild": "^0.24.0",
    "typescript": "^5.5.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
